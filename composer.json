{
  "name": "tourze/quic-crypto",
  "description": "QUIC协议加密库 - AEAD加密、包头保护、密钥派生",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "ext-openssl": "*",
    "ext-sodium": "*",
    "tourze/quic-core": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\QUIC\\Crypto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\QUIC\\Crypto\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}